{
  "schemaVersion": 1,
  "id": "tweakeroo-ext",
  "version": "${version}",
  "name": "Tweakeroo-ext",
  "description": "Extension mod for Tweakeroo",
  "authors": [
    "lovelellter"
  ],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/tweakeroo-ext/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.github.shu.tweakerooExt.client.TweakerooExtClient"
    ],
    "main": [
      "com.github.shu.tweakerooExt.TweakerooExt"
    ]
  },
  "mixins": [
    "tweakeroo-ext.mixins.json",
    "tweakeroo-ext-tweakeroo.mixins.json",
    "tweakeroo-ext-essentialgui.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "minecraft": "${minecraft_version}"
  }
}
